   float:right;
  }

</style>
<body class="">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
<link rel="stylesheet" href="https://bootswatch.com/cosmo/bootstrap.min.css">
<div class="container-fluid">
<div>
<div class="mainbody">
    <div class="row">
        <div class="navbar-wrapper">
            <div class="container-fluid">
                <div class="navbar navbar-default navbar-static-top" role="navigation">
                    <div class="container-fluid">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span><span
                                    class="icon-bar"></span><span class="icon-bar"></span>
                            </button>
                            <a class="navbar-brand" href="./ORqmj" style="margin-right:-8px; margin-top:-5px;">
                                <img alt="Brand" src="https://lut.im/7trApsDX08/GeilMRp1FIm4f2p7.png" width="30px" height="30px">
                            </a>
                            <a class="navbar-brand" href="./ORqmj">BCRS</a>
                            <i class="brand_network"><small><small>Bhawan Complaint Register System</small></small></i>
                        </div>
                        <div class="navbar-collapse collapse">
                            <ul class="nav navbar-nav">
                                <li><a href="/complaint">Complaint</a></li>
                            </ul>
                            <ul class="nav navbar-nav navbar-right">
                                <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <span class="user-avatar pull-left" style="margin-right:8px; margin-top:-5px;">
                                        <img src="https://lut.im/7JCpw12uUT/mY0Mb78SvSIcjvkf.png" class="img-responsive img-circle" title="John Doe" alt="John Doe" width="30px" height="30px">
                                    </span>
                                    <span class="user-name">
                                      {{user.username}}
                                    </span>
                                    <b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li>
                                            <div class="navbar-content">
                                                <div class="row">
                                                    <div class="col-md-5">
                                                        <img src="https://lut.im/7JCpw12uUT/mY0Mb78SvSIcjvkf.png" alt="Alternate Text" class="img-responsive" width="120px" height="120px" />
                                                        <p class="text-center small">
                                                            <a href="/api/photo" >change photo</a></p>
                                                    </div>
                                                    <div class="col-md-7">
                                                        <span>{{user.username}}</span>
                                                        <p class="text-muted small">
                                                            {{user.enrollment}}</p>
                                                        <div class="divider">
                                                        </div>
                                                        <a href="#" class="btn btn-default btn-xs"><i class="fa fa-user-o" aria-hidden="true"></i> Profile</a>
                                                        <a href="/users/forgot" class="btn btn-default btn-xs"><i class="fa fa-address-card-o" aria-hidden="true"></i>Forgot Password</a>
                                                        <a href="/users/update/profile" class="btn btn-default btn-xs"><i class="fa fa-cogs" aria-hidden="true"></i> Settings</a>
                                                        <a href="#" class="btn btn-default btn-xs"><i class="fa fa-question-circle-o" aria-hidden="true"></i> Help!</a>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="navbar-footer">
                                                <div class="navbar-footer-content">
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <a href="/users/update/profile#changed" class="btn btn-default btn-sm"><i class="fa fa-unlock-alt" aria-hidden="true"></i> Change Passowrd</a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <a href="/users/logout" class="btn btn-default btn-sm pull-right"><i class="fa fa-power-off" aria-hidden="true"></i> Sign Out</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div style="padding-top:50px;"> </div>
        <div class="col-lg-3 col-md-3 hidden-sm hidden-xs">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="media">
                        <div align="center">
                            <img class="thumbnail img-responsive" src="https://lut.im/7JCpw12uUT/mY0Mb78SvSIcjvkf.png" width="300px" height="300px">
                        </div>
                        <div class="media-body"><center>
                            <hr>
                            <h4><i class="fa  fa-user">&nbsp;&nbsp;&nbsp;</i><strong>{{user.username}}</strong></h4>
                            <hr>
                            <h4><i class="fa  fa-building">&nbsp;&nbsp;&nbsp;</i><strong>{{user.room}}</strong></h4>
                            <hr>
                            <h4><i class="fa  fa-mortar-board"></i>&nbsp;&nbsp;&nbsp;<strong>{{user.enrollment}}</strong></h4>
                            <hr>
                            <h4><i class="fa  fa-envelope">&nbsp;&nbsp;&nbsp;</i><strong>&nbsp;&nbsp;&nbsp; {{user.email}} </strong></h4>
                            <hr>
                            <h4><i class="fa  fa-phone">&nbsp;&nbsp;&nbsp;</i><strong>{{user.phone}}</strong></h4></center>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-9 col-md-9 col-sm-12 col-xs-12">
            <div class="panel panel-default">
                <div class="panel-body">
                    <span>
                        <h1 class="panel-title pull-left" style="font-size:30px;">{{user.username}} <small>{{user.enrollment}}</small> </h1>
                    </span>
                    <br><br><hr>
                    <span class="pull-left">
                        <a href="#" class="btn btn-link" style="text-decoration:none;"><i class="fa fa-fw fa-user-circle" aria-hidden="true"></i> Complaints <span class="badge">2</span></a>
                    </span>
                </div>
            </div>
            <hr>
            {{#each data}}
            {{#if this.completedUser}}<div class="panel panel-success">
              {{else}}<div class="panel panel-danger">{{/if}}
              <div class="panel-heading">
                    <div class="pull-left">
                        <a href="#">
                            <img class="media-object img-circle" src="https://lut.im/7JCpw12uUT/mY0Mb78SvSIcjvkf.png" width="50px" height="50px" style="margin-right:8px; margin-top:-5px;">
                        </a>
                    </div>
                    <h4><a href="#" style="text-decoration:none; color:white;"><strong>{{user.username}}</strong></a> – <small><small><a href="#" style="text-decoration:none; color:white;"><i><i class="fa fa-clock-o" aria-hidden="true"></i> 42 minutes ago</i></a></small></small></h4>
              </div>
<div class="panel-body">
<span class ="alert">
    {{#if this.completedAdmin}}
   Stated complete by Admin {{this.username}}
    {{/if}}
  </span>
                    <hr>
                    <div class="post-content float-left">
                        {{this.complaint}}




                        {{#if this.completedUser}}

                        {{else}}
                        <form class="form-inline kk" action="/users/change" method="post">
                           <input type="hidden" id = "o" name="complid" value={{this.id}}>
                          <label for="sel1">status :</label>

                          <select class="form-control"  name="check">
                            <option value="false">Not Completed</option>
                            <option value="true">Completed</option>


                          </select>
                          <button type="submit" class="btn btn-danger" name="button">submit</button>
                        </form>


                        {{/if}}





                    </div>
                    <hr>

</div>                    <div class="panel-footer">
                      {{#if this.completedAdmin}}
                          {{#if this.completedUser}}
                      <div class="pull-right btn-group-xs">
                          {{#if this.completedUser}}<i class="fa fa-check" style="color:green; font-size:28px;"></i>{{/if}}
                      </div>
                          {{/if}}
                      {{/if}}


                        <div class="pull-left">
                            <p class="text-muted" style="margin-left:5px;"><i class="fa fa-globe" aria-hidden="true"></i> {{this.compType}}</p>
                        </div>
                        <br>
                    </div>

                </div>

       {{/each}}

            <!-- Simple post content example. -->

<!-- this is the end -->

                </div>
            </div>

        </div>
    </div>
</div>



</body>
</html>
